{
  "name": "intelligence/wpos-pro-installer",
  "description": "An install helper for WP Migrate DB Pro and WP Migrate DB Pro Media Files",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Per Sturesson",
      "email": "per@persturesson.com"
    }
  ],
  "keywords": [
    "wordpress", "composer", "wp", "plugin", "wp offload s3 pro", "env", "pro"
  ],
  "require": {
    "php": ">=5.5",
    "composer-plugin-api": "^1.0",
    "vlucas/phpdotenv": "^2.2"
  },
  "require-dev": {
    "composer/composer": "1.0.*",
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.*",
    "satooshi/php-coveralls": "1.*"
  },
  "autoload": {
    "psr-4": { "IgniteOnline\\WPOSProInstaller\\": "src/WPOSProInstaller" }
  },
  "extra": {
    "class": "IgniteOnline\\WPOSProInstaller\\Plugin"
  },
  "scripts": {
    "lint": "phpcs src tests --standard=PSR2",
    "test": "phpunit",
    "coverage": "phpunit --coverage-html coverage",
    "coveralls": "coveralls"
  }
}
